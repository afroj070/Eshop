{
    "name":"firoj/eshop",
    "description": "ecommerce opence project",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "doctrine/orm": "^2.5.11",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "symfony/console": "^3.4",
        "symfony/expression-language": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.4",
        "eshop/backend": "dev-master",
        "eshop/frontend": "dev-master",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/yaml": "^3.4",
        "symfony/serializer": "^3.4"
    },
    "require-dev": {
        "symfony/dotenv": "^3.4",
        "symfony/var-dumper": "^3.4",
        "symfony/web-server-bundle": "^3.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/afroj070/Backend.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/afroj070/Frontend.git"
        }
       
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
}